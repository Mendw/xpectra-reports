<table>
    <tr>
        <% columns.forEach(column => { %>
            <th><%= column.name %></th>
        <% }) %>
    </tr>
    <% rows.forEach(row => { %>
        <tr>
            <% columns.forEach(column => { %>
                <td><%= row[column.name] %></td>
            <% }) %>
        </tr>
    <% }) %>
</table>